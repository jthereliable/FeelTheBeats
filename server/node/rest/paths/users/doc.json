			"parameters": [
				{
					"name": "name",
					"notes": "Search user based on name"
				},
				{
					"name": "tier",
					"notes": "Search user based on tier"
				},
				{
					"name": "mod_level",
					"notes": "Search user based on mod level"
				},
				{
					"name": "charter_level",
