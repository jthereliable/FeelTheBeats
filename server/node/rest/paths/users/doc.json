					"example": "'name', 'tier', 'points', 'experience'"
				},
				{
					"name": "sort",
					"notes": "Sorting direction",
					"example": "'asc' or 'desc'"
				},
				{
					"name": "offset",
					"notes": "Search offset"
				},
				{
					"name": "limit",
