{
	"title": "Retrieve list of artists",
	"path": "/groups/!gid/requests/?",
	"methods": [
		{
			"method": "get",
			"notes": "Get list of artists based on search parameters.",
			"parameters": [
				{
					"name": "token",
					"notes": "User token.",
					"required": true
				},
				{
					"name": "name",
					"notes": "Search artists based on name"
				},
				{
					"name": "linked",
					"notes": "Search artists linked with a user account"
				},
				{
					"name": "order",
					"notes": "Sort by field",
					"example": "'name'"
				},
				{
					"name": "sort",
					"notes": "Sorting direction",
					"example": "'asc' or 'desc'"
				},
				{
					"name": "offset",
					"notes": "Search offset"
				},
				{
					"name": "limit",
					"notes": "Search limit (max 25)"
				}
			],
			"success": {
				"notes": "Success output notes",
				"example": {
					"success": true,
					"artists": [
						"..."
					],
					"count": 1
				}
			},
			"error": {
				"notes": "Error output notes",
				"example": {
					"success": false,
					"error": "error message",
					"code": 1
				}
			}
		}
	]
}